{
  "name": "bowtie",
  "version": "2024.6.1",
  "description": "The UI displays the outcomes generated by Bowtie, which serves as a meta-validator for the JSON Schema specification",
  "repository": "github:bowtie-json-schema/bowtie",
  "license": "MIT",
  "author": {
    "name": "Julian Berman",
    "email": "Julian+Bowtie@GrayVines.com"
  },
  "keywords": [
    "jsonschema",
    "json schema",
    "json",
    "validation",
    "benchmarking",
    "apis",
    "api",
    "specifications"
  ],
  "type": "module",
  "engines": {
    "node": ">=21.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "auditConfig": {
      "ignoreGhsas": ["GHSA-67mh-4wv8-2f99"]
    }
  },
  "scripts": {
    "start": "vite --no-clearScreen",
    "build": "tsc && vite build",
    "bundle-visualizer": "vite-bundle-visualizer",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0 src",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test": "vitest run; playwright test",
    "watch-test": "vitest watch"
  },
  "dependencies": {
    "@testing-library/react": "^16.2.0",
    "bootstrap": "^5.3.3",
    "dayjs": "^1.11.13",
    "mathjs": "^14.3.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-bootstrap-icons": "^1.11.5",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.15.1",
    "title-case": "^4.3.2",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.50.1",
    "@types/eslint__js": "^9.14.0",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/urijs": "^1.19.25",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "recharts": "2.15.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^3.0.7"
  }
}

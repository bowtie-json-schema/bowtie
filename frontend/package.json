{
  "name": "bowtie",
  "version": "2025.5.1",
  "description": "The UI displays the outcomes generated by Bowtie, which serves as a meta-validator for the JSON Schema specification",
  "repository": "github:bowtie-json-schema/bowtie",
  "license": "MIT",
  "author": {
    "name": "Julian Berman",
    "email": "Julian+Bowtie@GrayVines.com"
  },
  "keywords": [
    "jsonschema",
    "json schema",
    "json",
    "validation",
    "benchmarking",
    "apis",
    "api",
    "specifications"
  ],
  "type": "module",
  "engines": {
    "node": ">=21.0.0"
  },
  "packageManager": "pnpm@10.22.0",
  "pnpm": {
    "auditConfig": {
      "ignoreGhsas": [
        "GHSA-67mh-4wv8-2f99"
      ]
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "scripts": {
    "start": "vite --no-clearScreen",
    "build": "tsc && vite build",
    "bundle-visualizer": "vite-bundle-visualizer",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0 src",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test": "vitest run; playwright test",
    "watch-test": "vitest watch"
  },
  "dependencies": {
    "@testing-library/react": "^16.3.1",
    "bootstrap": "^5.3.8",
    "dayjs": "^1.11.19",
    "mathjs": "^15.1.0",
    "react": "^19.2.3",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^3.6.0",
    "title-case": "^4.3.2",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/urijs": "^1.19.26",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-tsdoc": "^0.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "recharts": "3.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^4.0.16"
  },
  "resolutions": {
    "prismjs": "^1.30.0"
  }
}

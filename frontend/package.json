{
  "name": "bowtie",
  "version": "2025.5.1",
  "description": "The UI displays the outcomes generated by Bowtie, which serves as a meta-validator for the JSON Schema specification",
  "repository": "github:bowtie-json-schema/bowtie",
  "license": "MIT",
  "author": {
    "name": "Julian Berman",
    "email": "Julian+Bowtie@GrayVines.com"
  },
  "keywords": [
    "jsonschema",
    "json schema",
    "json",
    "validation",
    "benchmarking",
    "apis",
    "api",
    "specifications"
  ],
  "type": "module",
  "engines": {
    "node": ">=21.0.0"
  },
  "packageManager": "pnpm@10.15.0",
  "pnpm": {
    "auditConfig": {
      "ignoreGhsas": [
        "GHSA-67mh-4wv8-2f99"
      ]
    }
  },
  "scripts": {
    "start": "vite --no-clearScreen",
    "build": "tsc && vite build",
    "bundle-visualizer": "vite-bundle-visualizer",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0 src",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test": "vitest run; pnpm exec playwright install; playwright test",
    "watch-test": "vitest watch"
  },
  "dependencies": {
    "@testing-library/react": "^16.3.0",
    "bootstrap": "^5.3.7",
    "dayjs": "^1.11.13",
    "mathjs": "^14.6.0",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.1.1",
    "react-router": "^7.7.1",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.15.4",
    "title-case": "^4.3.2",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/urijs": "^1.19.25",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "recharts": "2.15.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.3.6",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "prismjs": "^1.30.0"
  }
}
